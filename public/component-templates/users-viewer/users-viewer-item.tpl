<div class="user-viewer-item">
    <div class="row header">
        <img src="{{object.avatar}}" class="img-circle col-sm-4" alt="">
        <a href="/user/{{object.username}}" class="user-name col-sm-8">{{object.name}}</a>
    </div>
    <div class="row body">
        <small class="row">{{object.tagline}}</small>
    </div>
</div>